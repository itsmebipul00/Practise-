{
    "manifest_version":2,
    "name": "Tic",
    "version": "1.0.0",
    "description": "Tic Tac Toe on Every URL",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js":["copy.js"]
        }
    ],
    "browser_action": {
        "default_popup": "copy.html",
        "default_icon": "logo.png"
    } 
}